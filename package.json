{
  "name": "@cakeui/react",
  "version": "3.0.3",
  "description": "A React UI library designed for intuitive use while providing deep customization capabilities.",
  "license": "MIT",
  "author": "cakeui",
  "homepage": "https://cakeui.com",
  "repository": "github:cakeuidev/cakeui",
  "type": "module",
  "files": ["dist/lib"],
  "exports": {
    ".": "./dist/lib/index.js",
    "./styles.css": "./dist/lib/styles.css"
  },
  "scripts": {
    "dev": "node index.js",
    "build": "vite build && vite build -c vite.ssr.config.ts",
    "build:lib": "vite build -c vite.lib.config.ts",
    "preview": "cross-env NODE_ENV=production node index.js",
    "deploy": "gh-pages -d dist/src -f --nojekyll --cname www.cakeui.com",
    "prepublishOnly": "npm run build:lib"
  },
  "dependencies": {
    "dayjs": "^1.11.19"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@types/compression": "^1.8.1",
    "@types/css-modules": "^1.0.5",
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "compression": "^1.8.1",
    "cross-env": "^10.1.0",
    "express": "^5.1.0",
    "gh-pages": "^6.3.0",
    "shiki": "^3.14.0",
    "sirv": "^3.0.2",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4"
  }
}
